{
  "name": "flow-higher",
  "version": "5.0.3",
  "description": "Higher-kinded types for Flow",
  "main": "lib/main.js",
  "files": [
    "lib/"
  ],
  "repository": {
    "url": "git@github.com:tkuminecz/flow-higher.git",
    "type": "git"
  },
  "author": "Tim Kuminecz <tkuminecz@gmail.com>",
  "license": "MIT",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "build": "make clean && make",
    "dev": "babel-node",
    "flow": "flow",
    "test": "npm-run-all flow test:cover",
    "test:unit": "BABEL_ENV=test babel-tape-runner $(\\find test | \\grep \\.js) | tap-diff",
    "test:cover": "nyc npm run test:unit && nyc report --reporter=html",
    "test:coveralls": "nyc npm run test:unit && nyc report --reporter=text-lcov | coveralls",
    "travis": "npm-run-all flow test:coveralls"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-tape-runner": "^2.0.1",
    "coveralls": "^2.11.15",
    "eslint-config-proper": "^0.5.0",
    "flow-bin": "0.39.0",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "flow-type-list": "3.0.0"
  }
}
